@model CommitDetailsViewModel
<div class="row">
  <div class="large-12 columns">
    <h1 class="small-headline">@Model.RepositoryPath</h1>
  </div>
</div>
@foreach (var file in Model.Files)
{
  <div class="row">
    <div class="large-12 columns">
      <h5>@file.Path</h5>
    </div>
  </div>
  <div class="row">
    <div class="large-12 columns">
      <pre class="prettyprint">@file.Patch</pre>
    </div>
  </div>
}